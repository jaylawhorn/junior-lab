void test_water() {

  TString conf="water_n2.txt";

  vector<TString> fnamev;
  vector<Int_t> timev;
  vector<Double_t> maxv;
  vector<TGraphErrors*> graphv;

  confParse(conf, fnamev, timev, graphv);

  for (Int_t i=0; i<fnamev.size(); i++) {
    
    getPeaks(fnamev[i], timev[i], i, "water_n2", graphv[i]);

  }

}
